<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="PrimaryButton">
        <item name="android:background">@drawable/zy_button_ripple_primary</item>
        <item name="android:fontFamily">@font/rajdhani_medium</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/zyWhite</item>
        <item name="android:paddingLeft">6dp</item>
        <item name="android:paddingRight">6dp</item>
    </style>

    <style name="TextViewMedium">
        <item name="android:gravity">center_vertical</item>
        <item name="fontFamily">@font/rajdhani_medium</item>
        <item name="android:textColor">@color/zyTextColorPrimary</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">16sp</item>
    </style>

</resources>